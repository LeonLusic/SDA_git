exp <- function(n, rate) {
x = rexp(n, rate)
title = paste("Histogram vs. Density\n", "n =", n, ", rate =", rate)
hist(x, freq = FALSE, col="white", border="black",main=title)
points = seq(0, max(x), length=length(x))
lines(points, dexp(points,rate), col="red")
}
